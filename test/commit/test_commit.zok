from "utils/pack/unpack128.zok" import main as unpack128
from "utils/pack/pack128.zok" import main as pack128

def main(field input1, field input2) -> (field[2]):
    i1 = unpack128(input1)
    i2 = unpack128(input2)
    field[256] o = [0;256]
    for field i in 0..128 do
        o[i] = i1[i]
        o[128+i] = i2[i]
    endfor
    o1 = pack128(o[0..128])
    o2 = pack128(o[128..256])
    return [o1, o2]